package ch.hearc.p2.game.level.tile;

public class Tile {
	protected int x;
	protected int y;
	protected BoundingShape boundingShape;

	public Tile(int x, int y) {
		this.x = x;
		this.y = y;
		this.boundingShape = null;
	}

	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}
}
